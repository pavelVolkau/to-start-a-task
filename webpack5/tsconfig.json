{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "ES2022",
    "module": "ES2022",
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
  },
  "baseUrl": "src",
}